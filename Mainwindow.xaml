<Window x:Class="U5Printing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U5Printing"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Rectangle Stroke="White" StrokeThickness="100" Height="100"/>
        <Label Height="40"/>
        <Label Content="Input the number of copies you wish to print."/>
        <TextBox x:Name="txtInput" Height="20" Width="100"/>
        <Button Height="20" Width="300" x:Name="btnRun" Content="Your Printing cost calculator" Click="btnRun_Click_1" />
        <Label x:Name= "lblOutput" Content="Your Printing cost is:"/>
    </StackPanel>
</Window>
